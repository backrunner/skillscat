{
  "name": "@skillscat/web",
  "version": "0.1.11",
  "type": "module",
  "scripts": {
    "build:sw": "node scripts/build-sw.mjs",
    "dev": "pnpm build:sw && vite dev",
    "build": "pnpm build:sw && vite build",
    "prepare:assetsignore": "node scripts/prepare-assetsignore.mjs",
    "preview": "node scripts/preview.mjs",
    "typecheck": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint .",
    "deploy": "pnpm build && pnpm prepare:assetsignore && wrangler deploy -c wrangler.preview.toml --env production",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply skillscat-db --local -c wrangler.preview.toml",
    "db:migrate:prod": "wrangler d1 migrations apply skillscat-db --remote -c wrangler.preview.toml --env production",
    "db:studio": "drizzle-kit studio",
    "tags:normalize": "node scripts/normalize-skill-tags.mjs"
  },
  "dependencies": {
    "@cf-wasm/resvg": "^0.3.3",
    "@hugeicons/core-free-icons": "^3.1.1",
    "@hugeicons/svelte": "^1.1.1",
    "better-auth": "^1.4.18",
    "bits-ui": "^2.15.5",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "marked": "^17.0.2",
    "shiki": "^3.22.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260214.0",
    "@sveltejs/adapter-cloudflare": "^7.2.7",
    "@sveltejs/kit": "^2.51.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@unocss/preset-icons": "^66.6.0",
    "@unocss/preset-wind": "^66.6.0",
    "@unocss/reset": "^66.6.0",
    "@unocss/svelte-scoped": "^66.6.0",
    "@unocss/transformer-directives": "^66.6.0",
    "drizzle-kit": "^0.31.9",
    "esbuild": "^0.27.3",
    "svelte": "^5.51.0",
    "svelte-check": "^4.4.0",
    "typescript": "^5.9.3",
    "unocss": "^66.6.0",
    "vite": "^7.3.1",
    "wrangler": "^4.65.0"
  }
}
