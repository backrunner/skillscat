{
  "name": "skillscat",
  "version": "0.1.0",
  "description": "CLI for installing AI agent skills from SkillsCat registry",
  "type": "module",
  "bin": {
    "skillscat": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^22.19.11",
    "rollup": "^4.57.1",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "claude",
    "claude-code",
    "skills",
    "cli",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skillscat/skillscat"
  },
  "homepage": "https://skillscat.com"
